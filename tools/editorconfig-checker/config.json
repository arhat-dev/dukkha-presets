{
  "Verbose": false,
  "Debug": false,
  "IgnoreDefaults": false,
  "SpacesAftertabs": false,
  "NoColor": false,
  "Exclude": [
    "\\.git/.*",
    "private/.*",
    "vendor/.*",
    "generated/.*",
    "build/.*",
    ".dukkha/.*",
    ".*\\.tar.*",
    ".*\\.zip.*",
    ".*\\.rar.*",
    "third_party/golang/.*doc.go",
    "testdata.*"
  ],
  "AllowedContentTypes": [],
  "Disable": {
    "EndOfLine": false,
    "Indentation": false,
    "InsertFinalNewline": false,
    "TrimTrailingWhitespace": false,
    "IndentSize": false,
    "MaxLineLength": false
  }
}
