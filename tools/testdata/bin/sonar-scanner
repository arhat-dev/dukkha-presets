    echo "INFO: SonarScanner 4.7.2.2472"
    echo "INFO: Java 11.0.12 Homebrew (64-bit)"
    echo "INFO: Mac OS X 11.6.1 aarch64"
  )
  ;;
*)
  echo "Run sonar-scanner"
  ;;
esac
